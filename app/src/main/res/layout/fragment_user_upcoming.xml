<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_light_blue"
    android:layout_gravity="center"
    android:orientation="vertical"
    android:id="@+id/userUpcomingFragment"
    tools:context="view.fragment.UserUpcomingFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/user_upcoming_recylerview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:listitem="@layout/user_single_upcoming">

    </androidx.recyclerview.widget.RecyclerView>

</FrameLayout>